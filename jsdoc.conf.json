{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "systemSummary": "Augmented.js Next Presentation - Simply Development.",
    "systemColor": "#607D8B",
    "systemLogo": "public/images/jensen-silo-circle-small.png",
    "logoFile": "public/images/jensen-silo-circle-small.png",
    "cleverLinks": false,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "Augmented.js Next Presentation",
    "footer": "<a href='http://www.augmentedjs.com'>Augmented.js Homepage</a>",
    "copyright": "",
    "navType": "vertical",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "protocol": "html://",
    "methodHeadingReturns": false,
    "favicon": "public/images/favicon-32x32.png",
    "showTableOfContents": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "source": {
    "include": [
      "./node_modules/presentation-application/src",
      "./node_modules/presentation-components/src",
      "./node_modules/presentation-controller/src",
      "./node_modules/presentation-decorator/src",
      "./node_modules/presentation-dom/src",
      "./node_modules/presentation-form/src",
      "./node_modules/presentation-logger/src",
      "./node_modules/presentation-mediator/src",
      "./node_modules/presentation-models/src",
      "./node_modules/presentation-request/src",
      "./node_modules/presentation-router/src",
      "./node_modules/presentation-storage/src",
      "./node_modules/presentation-table/src",
      "./node_modules/presentation-view/src",
      "./node_modules/presentation-widget/src"
    ],
    "exclude": [
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  }
}
