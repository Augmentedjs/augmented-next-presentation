{
  "name": "augmentedjs-next-presentation",
  "version": "2.0.0-alpha.13",
  "description": "The Augmented.js Next - Presentation Module.",
  "main": "dist/augmented-next-presentation.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --content-base ./ --env.dev",
    "build": "webpack --env.prod",
    "lint": "eslint ./src/augmented-presentation.js",
    "setup": "npm install && npm run validate",
    "validate": "npm-run-all lint test build",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "start": "node index.js",
    "docs": "jsdoc ./src -r -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/augmented-presentation-next.git"
  },
  "keywords": [
    "augmentedjs",
    "presentation"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.11.6",
    "chai": "^4.1.2",
    "eslint": "^3.2.2",
    "eslint-config-kentcdodds": "^9.0.2",
    "express": "^4.16.2",
    "foodoc": "0.0.8",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.1.0",
    "lodash.bind": "^4.2.1",
    "lodash.pick": "^4.4.0",
    "mocha": "^4.1.0",
    "mocha-jsdom": "^1.1.0",
    "node-localstorage": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "sinon": "^4.1.4",
    "webpack": "^2.1.0",
    "webpack-config-utils": "^2.0.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.10.0",
    "webpack-validator": "^2.2.7"
  },
  "dependencies": {
    "augmentedjs-next": "^2.0.0-alpha.18",
    "lodash.isregexp": "^4.0.1",
    "lodash.map": "^4.6.0"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/augmented-presentation-next/issues"
  },
  "homepage": "http://www.augmentedjs.com"
}
