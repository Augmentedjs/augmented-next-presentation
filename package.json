  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^3.2.2",
    "eslint-config-kentcdodds": "^9.0.2",
    "express": "^4.16.3",
    "foodoc": "0.0.8",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.6.2",
    "mocha": "^4.1.0",
    "mocha-jsdom": "^1.1.0",
    "node-localstorage": "^1.3.1",
    "sinon": "^4.4.8",
