{
  "name": "augmentedjs-next-presentation",
  "version": "2.6.0",
  "description": "The Augmented.js Next - Presentation Module.",
  "main": "dist/augmented-next-presentation.js",
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader --progress",
    "build": "webpack --mode production --module-bind js=babel-loader --progress",
    "lint": "eslint ./src/augmented-presentation.js",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "start": "node index.js",
    "docs": "rm -rf jsdoc && jsdoc ./src -r -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json -P ./package.json",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/augmented-next-presentation.git"
  },
  "keywords": [
    "augmentedjs",
    "presentation"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^3.2.2",
    "eslint-config-kentcdodds": "^9.0.2",
    "express": "^4.16.4",
    "foodoc": "0.0.9",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.12.0",
    "mocha": "^4.1.0",
    "mocha-jsdom": "^1.2.0",
    "node-localstorage": "^1.3.1",
    "sinon": "^4.5.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "augmentedjs-next": "^2.2.1",
    "presentation-application": "^2.5.6",
    "presentation-components": "^2.7.3",
    "presentation-controller": "^2.5.4",
    "presentation-decorator": "^2.5.5",
    "presentation-dom": "^2.8.1",
    "presentation-form": "^2.7.2",
    "presentation-logger": "^2.5.4",
    "presentation-mediator": "^2.5.8",
    "presentation-models": "^2.6.1",
    "presentation-request": "^2.6.1",
    "presentation-router": "^2.6.5",
    "presentation-storage": "^2.5.6",
    "presentation-table": "^2.5.6",
    "presentation-view": "^2.5.5",
    "presentation-widget": "^2.5.5"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/augmented-next-presentation/issues"
  },
  "homepage": "http://www.augmentedjs.com"
}
